#List practice mutable
'''
a = ['apple', 'orange', 'banana']

print(a[1])
'''

'''
b = ['India', 'Nepal', 'Tibet']
b[0] = 'Germany'

print(b)
'''

#Tuples Practice immutable
''''
c = ('Africa', 'Indonesia', 'tanzania')

print(c)
'''

#Dictionary immutable
'''
a = {'Europe'}
b = {'Poland'}
c = {'Russia'}

print(a,b,c)

'''

#Sets unordered
'''
D = {'apple','banana', 'cherry','Avacado'}
print(D)
'''

#Sets removes duplicates
''''
E = {'orange', 'coconut', 'Mango', 'coconut'}
print(E)
'''

#Sets union
'''
F = {'a', 'b', 'c'}
G = {'d', 'e', 'f'}
x =set.union(F,G)
print(x)
'''

#sets intersection
'''
H = {'a', 'b', 'c'}
I = ('c','d','f', 'a')
z = H.intersection(I)
print(z)
'''

#convert a set to list
'''
J = {'a', 'b', 'c'}
print(list[J])
'''

#Slicing
'''
a = ['b','c','d','e','f']
print(a[:-1])
'''

#indexing
'''
a = ['a','s','d','f','g']
print(a[0])
'''

#extend

a = ['l','k','j','h','j']
b = a.extend(['m'])
print(a)
